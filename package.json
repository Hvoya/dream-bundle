{
  "name": "vue-dream-bundle",
  "version": "0.0.1",
  "description": "Dream bundle boilerplate for vue project",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js",
    "docs:generate": "jsdoc webpack src -d docs -r && cd ./docs && http-server -c-1 -o",
    "analyze-bundle-size": "cross-env NODE_ENV=production analyze=true webpack --config webpack/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "https://github.com/Hvoya",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano-webpack-plugin": "^1.0.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.3.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^8.0.0",
    "imagemin-webpack": "^5.1.1",
    "jsdoc": "^3.6.4",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "vue": "^2.6.11"
  }
}
